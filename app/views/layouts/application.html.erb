<!DOCTYPE html>
<html>
<head>
  <title>Doccheck</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
	
<body>
	<div class="container">
		<%if user_signed_in? %>
			<p><%= current_user.email%></p>
		<%end%>
		<%= yield %>
	</div>
</body>
</html>

  


